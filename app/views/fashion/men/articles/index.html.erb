<%= stylesheet_link_tag 'fashion-article' %>
<%= render "nav_bar" %>
<div class="fashion">
	<h1>Men's Fashion Trends</h1>
		<% @article.each  do |article| %>
		<div class="article"> 
			<% if article.category == "mfashion" %>
				<h3><%= article.title %></h3>
				<h3> <a href="<%= article.url %>" target="blank"> <%= article.source %></a></h3>
				<img src="<%=article.image%>"/>
			<% end %>
		<% end %>
	</div>
